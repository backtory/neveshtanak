<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <corners android:radius="2dip" />

            <stroke
                android:width="1dip"
                android:color="#E5E5E5" />
        </shape>
    </item>
    <item
        android:bottom="1dip"
        android:left="1dip"
        android:right="1dip"
        android:top="1dip">
        <shape android:shape="rectangle" >
            <corners android:radius="2dip" />

            <solid android:color="#ffffff" />
        </shape>
    </item>

</layer-list>